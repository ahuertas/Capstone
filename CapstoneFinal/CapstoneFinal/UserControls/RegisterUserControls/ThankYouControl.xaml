                Background="{DynamicResource AccentColorBrush}"/>
        <Label TextElement.FontSize="30" Content="Thank you for &#xD;&#xA;registering for NU Buddy" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="114,85,0,0" Width="377"/>

