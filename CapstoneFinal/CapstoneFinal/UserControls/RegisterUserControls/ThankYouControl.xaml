             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CapstoneFinal.UserControls.RegisterUserControls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="600">
    <Grid>
        <Label Content="Pg 8 - Final"  TextElement.FontSize="20"  TextElement.FontStyle="Italic" HorizontalAlignment="Left" Margin="466,23,0,0" VerticalAlignment="Top" Height="38" Width="114"/>

        <Button x:Name="Back" Content="Back" HorizontalAlignment="Left" Margin="90,376,0,0" VerticalAlignment="Top" Width="75" Click="Back_Click"
