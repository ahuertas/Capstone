<Window x:Class="Capstone_Base_2.View.RegisterViews.UserName"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Capstone_Base_2.View.RegisterViews"
        mc:Ignorable="d"
        Title="UserName" Height="300" Width="400"
        WindowStartupLocation="CenterScreen">
    <Grid>
        <Label Content="User Name" HorizontalAlignment="Left" Margin="21,26,0,0" VerticalAlignment="Top" Height="32" Width="112"/>
        <Label Content="password" HorizontalAlignment="Left" Margin="21,119,0,0" VerticalAlignment="Top" Width="112"/>
        <TextBox x:Name="UserName_TxtBox" HorizontalAlignment="Left" Height="23" Margin="21,63,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="363"/>
        <PasswordBox x:Name="password" HorizontalAlignment="Left" Margin="21,150,0,0" VerticalAlignment="Top" Width="363" Height="25"/>
        <Button x:Name="Back" Content="Back" HorizontalAlignment="Left" Margin="10,241,0,0" VerticalAlignment="Top" Width="75" Click="Back_Click" />
        <Button x:Name="Next" Content="Security" HorizontalAlignment="Left" Margin="309,241,0,0" VerticalAlignment="Top" Width="75" Click="Next_Click" />
    </Grid>
</Window>
